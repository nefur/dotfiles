{
    "env": {
        "browser": true,
        "jquery": true,
        "node": true
    },
     "rules": {
        "quotes": [2, "single"],
        "strict": 2,
        "block-scoped-var": 1,
        "no-spaced-func": 1,
        "space-after-keywords": [1, "always"],
        "brace-style": [2, "1tbs"],
        "curly": 2,
        "default-case": 1,
        "dot-notation": [1, {"allowKeywords": false}],
        "eqeqeq": 2,
        "wrap-iife": [2, "inside"],
        "no-underscore-dangle": 0
    }
}
